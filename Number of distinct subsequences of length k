#include <bits/stdc++.h>
#include <iostream>
#include <algorithm>
#include <vector>
#include <map>
#include <unordered_map>
#include <set>
#include <queue>
using namespace std;
#define pb push_back
#define mp make_pair
#define INF 1e9
#define mod 100
#define eps 1e-6
#define abs(x) ((x)>=0?(x):-(x))
#define y1 solai
#define fi first
#define se second
typedef long long ll;
void read(ll &x)
{
	scanf("%lld",&x);
}
void read(ll &x, ll &y)
{
	scanf("%lld%lld",&x,&y);
}
void read(ll &x, ll &y, ll &z)
{
	scanf("%lld%lld%lld",&x,&y,&z);
}
void print(ll x)
{
	printf("%lld ",x);
}
void println(ll x)
{
	printf("%lld\n",x);
}
const ll N=500;
ll n,k,d[N][N],p[N][N],le[N],ans,pos[N],last;
char s[N];
int main()
{
	cin>>n>>k;
	scanf("\n%s",s);
	for(ll i=1;i<=n;i++)
	{
		le[i]=pos[s[i-1]];
		pos[s[i-1]]=i;
	}
	for(ll j=1;j<=n;j++)
	{
		for(ll i=j;i<=n;i++)
		{
			last=le[i];
			if(last>0)
				d[i][j]=p[i-1][j-1]-p[last-1][j-1];
			else
				d[i][j]=p[i-1][j-1];
			if(last==0&&j==1)
				d[i][j]++;
			d[i][j]=min(d[i][j],100LL);
			p[i][j]=p[i-1][j]+d[i][j];
		}
	}
	for(ll len=n;len>=1;len--)
		for(ll i=1;i<=n;i++)
			if(d[i][len]<k)
				k-=d[i][len],ans+=(n-len)*d[i][len];
			else
				ans+=(n-len)*k,k=0,i=n,len=1;
	if(k>0)
		k--,ans+=n;
	if(k>0)
		ans=-1;
	println(ans);
}
